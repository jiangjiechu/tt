#nop 任务;
#class dba open;
#var f1_set 0;
#var f2_set 0;

#tic {dba} {#untic dba;dbaa} {2};
#ali {dbaa} {
  #var in_db 1;
  set wimpy 20;
  tune bd;
  #var db_enemies {};
  ask yu about job};
#ali {dbd} {ask yu about finish;dbx;#var in_db 0};
#ali {dbf} {ask yu about fail;#untic dba1;dbx;#var in_db 0};
#ali {ah} {
  #var in_battle 0;
  #if {"$myid"=="simontuixue"} {p.yy;yun shield};
  say $anhao};
#ali {jt} {
  #untic dba1;#untic dba2;
  ask %1 about 接头};
#ali {r} { #var anhao %1; };

#act {^余d%*说道：为了保护接头人的安全，%*，我只能告诉你某人在%*等着你。$} {
  #var db_outlook %4;
  #tic {dba1} {#echo {%c%h} {light yellow} {线人在%3，长相%4}} {5}
};
#act {^余d%*说道：为了保护接头人的安全，%*，我只能告诉你某人在%*等着你。长相大致如下%*$} {
  #var db_outlook %4;
  #tic {dba1} {#echo {%c%h} {light yellow} {线人在%3，长相%4}} {5};
};
#act {^余d%*说道：为了保护接头人的安全，%*，我只能告诉你某人在%*等着你。长相大致如下%*\n%*$} {
  #var db_outlook %4;
  #tic {dba1} {#echo {%c%h} {light yellow} {线人在%3，长相%4}} {5}
};
#act {^长相大致如下%*} {
  #var db_outlook %4;
  #tic {dba2} {#echo {%c%h} {light yellow} {长相%1}} {5}
};

#act {%*大元 秘谍 %*(%*)%*} {
  #var oppo @lower{%3};
  #if {@ismember{$oppo;db_enemies}==0} {#list db_enemies add $oppo};
};
#nop 辛金生在你的耳边悄声说道：你到中原去点燃一支心星礼花，就会有人和你联络。;
#act {%*在你的耳边悄声说道：你到%*去点燃一支%*礼花，就会有人和你联络。} {
  #var lihua %3;
  #tic {dba2} {#echo {%c%h} {light yellow} {去%2点燃一支%3礼花}} {5}
};
#nop 这时就在不远处的大驿道，似乎有人放烟花回应，你寻思着这大约就是接头口令了。;
#nop 如果你没记错的话，接头人名唤南宫年。;
#act {这时就在不远处的%*，似乎有人放烟花回应，你寻思着这大约就是接头口令了。} {
  #var jt_name {};
  #tic {dba3} {#echo {%c%h} {light yellow} {接头人在%1,名字 $jt_name}} {5}
};
#act {如果你没记错的话，接头人名唤%*。} {
  #var jt_name %1;
};


#act {%*的%*，说出%*，%*会找你。} {
  #var anhao %3;
  #tic {dba2} {#echo {%c%h} {light yellow} {目标%4在%1的%2，说出%3}} {5}
};

#act {%*=====%*　　　　　　%*◎◎%*} {
  #var anhao %2;
  #tic {dba2} {#echo {%c%h} {light yellow} {目标%1在%3的%4，说出%2}} {5}
};
#act {%*lllll%*==%*z　z　z　z　%*} {
  #var anhao %1;
  #tic {dba2} {#echo {%c%h} {light yellow} {目标%2在%3的%4，说出%1}} {5}
};
#nop 左令林~　~　~　~　不得向着天下◇◇◇◇◇◇◇◇◇◇岳王墓●●●●●●●●忘忧亭;
#act {%*~　~　~　~　%*◇◇◇◇◇◇◇◇◇◇%*●●●●●●●●%*\n在其人面前说出(say)暗号。} {
  #var anhao %2;
  #tic {dba2} {#echo {%c%h} {light yellow} {目标%1在%3的%4，说出%2}} {5}
};
#nop 低声是不是悄声CCCCCCCCCC刘月；；；；；登州＿＿鼓楼;
#act {%*CCCCCCCCCC%*；；；；；%*＿＿%*} {
  #var anhao %1;
  #tic {dba2} {#echo {%c%h} {light yellow} {目标%2在%3的%4，说出%1}} {5}
};
#nop 翔龙东街nn广州府nnnn陈杰　　　　　闪烁皇宫推动;
#act {%*nn%*nnnn%*　　　　　%*} {
  #var anhao %4;
  #tic {dba2} {#echo {%c%h} {light yellow} {目标%3在%2的%1，说出%4}} {5}
};
#nop     五十岁侯通海决计    人在南昌，南大街附近，名唤童璧;
#act {%!s%S%!s人在%*，%*附近，名唤%*} {
  #var anhao %1;
  #tic {dba2} {#echo {%c%h} {light yellow} {目标%4在%2的%3，说出%1}} {5}
};
#nop 小胡同◇◇◇洛阳hhh于娴y　y　只是甚么此事;
#act {%*◇◇◇%*hhh%*y　y　%*} {
  #var anhao %4;
  #tic {dba2} {#echo {%c%h} {light yellow} {目标%3在%2的%1，说出%4}} {5}
};
#nop 老婆不敢十六七颚颚颚颚颚颚颚颚颚蚺顺３伞〓〓〓归云庄□□□□□太湖桥;
#act {%*颚颚颚颚颚颚颚颚颚%*〓〓〓〓%*□□□□□%*} {
  #var anhao %1;
  #tic {dba2} {#echo {%c%h} {light yellow} {目标%2在%3的%4，说出%1}} {5}
};
#nop 右手一口气裂开欧阳惠　　　　　姑苏慕容lllll小道;
#act {%*%*　　　　　%*lllll%*} {
  #var anhao %1;
